        <activity android:name=".Activities.ActivityGlutes" ></activity>
        <activity android:name=".Activities.ActivityHamstrings" ></activity>
        <activity android:name=".Activities.ActivityLats" ></activity>
        <activity android:name=".Activities.ActivityLowerBack" ></activity>
        <activity android:name=".Activities.ActivityQuads" ></activity>
        <activity android:name=".Activities.ActivityShoulders" ></activity>
        <activity android:name=".Activities.ActivityTraps" ></activity>
        <activity android:name=".Activities.ActivityTriceps" ></activity>
        <activity android:name=".Activities.ActivityUpperBack" ></activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>