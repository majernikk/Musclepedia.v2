        </activity>
        <activity android:name=".Activities.ActivityAbdominals" ></activity>
        <activity android:name=".Activities.ActivityBiceps" ></activity>
        <activity android:name=".Activities.ActivityCalves" ></activity>
        <activity android:name=".Activities.ActivityChest" ></activity>
