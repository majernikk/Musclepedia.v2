<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:translationZ="10dp"
    tools:context="sk.spse.musclepediav2.MainActivity">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="?attr/colorPrimary"
        android:minHeight="?attr/actionBarSize"
        android:onClick="lupa"
        app:theme="@style/ThemeOverlay.AppCompat.Dark" />

    <ImageView
        android:id="@+id/trapsRight"
        android:layout_width="18dp"
        android:layout_height="18dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="101dp"
        android:layout_marginTop="157dp"
        android:onClick="traps"
        android:translationZ="10dp"
        app:srcCompat="@drawable/trapsright"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/trapsLeft"
        android:layout_width="22dp"
        android:layout_height="18dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="73dp"
        android:layout_marginTop="157dp"
        android:onClick="traps"
        android:translationZ="10dp"
        app:srcCompat="@drawable/trapsleft"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/Pecs"
        android:layout_width="75dp"
        android:layout_height="29dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="60dp"
        android:layout_marginTop="176dp"
        android:onClick="pecs"
        android:translationZ="10dp"
        app:srcCompat="@drawable/pecs"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/deltoidsLeft"
        android:layout_width="31dp"
        android:layout_height="27dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="49dp"
        android:layout_marginTop="172dp"
        android:onClick="deltoids"
        android:translationZ="9dp"
        app:srcCompat="@drawable/deltoidsleft" />

    <ImageView
        android:id="@+id/deltoidsRight"
        android:layout_width="30dp"
        android:layout_height="29dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="115dp"
        android:layout_marginTop="171dp"
        android:onClick="deltoids"
        android:translationZ="9dp"
        app:srcCompat="@drawable/deltoidsright" />

    <ImageView
        android:id="@+id/bicepsLeft"
        android:layout_width="30dp"
        android:layout_height="35dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="40dp"
        android:layout_marginTop="196dp"
        android:onClick="biceps"
        android:translationZ="8dp"
        app:srcCompat="@drawable/bicepsleft" />

    <ImageView
        android:id="@+id/bicepsRight"
        android:layout_width="27dp"
        android:layout_height="35dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="127dp"
        android:layout_marginTop="196dp"
        android:onClick="biceps"
        android:translationZ="8dp"
        app:srcCompat="@drawable/bicepsright" />

    <ImageView
        android:id="@+id/obliques"
        android:layout_width="37dp"
        android:layout_height="44dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="78dp"
        android:layout_marginTop="203dp"
        android:onClick="obliques"
        android:translationZ="10dp"
        app:srcCompat="@drawable/obliques" />

    <ImageView
        android:id="@+id/forearmsLeft"
        android:layout_width="35dp"
        android:layout_height="67dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="22dp"
        android:layout_marginTop="212dp"
        android:onClick="forearms"
        android:translationZ="7dp"
        app:srcCompat="@drawable/forearmsleft" />

    <ImageView
        android:id="@+id/forearmsRight"
        android:layout_width="35dp"
        android:layout_height="57dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="137dp"
        android:layout_marginTop="218dp"
        android:onClick="forearms"
        android:translationZ="7dp"
        app:srcCompat="@drawable/forearmsright" />

    <ImageView
        android:id="@+id/quadsLeft"
        android:layout_width="48dp"
        android:layout_height="84dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="52dp"
        android:layout_marginTop="257dp"
        android:onClick="quads"
        android:translationZ="10dp"
        app:srcCompat="@drawable/quadsleft" />

    <ImageView
        android:id="@+id/quadsRight"
        android:layout_width="48dp"
        android:layout_height="83dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="94dp"
        android:layout_marginTop="257dp"
        android:onClick="quads"
        android:translationZ="11dp"
        app:srcCompat="@drawable/quadsright" />

    <ImageView
        android:id="@+id/calvesLeft"
        android:layout_width="33dp"
        android:layout_height="69dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="52dp"
        android:layout_marginTop="349dp"
        android:onClick="calves"
        android:translationZ="10dp"
        app:srcCompat="@drawable/calvesleft"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/calvesRight"
        android:layout_width="27dp"
        android:layout_height="67dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="112dp"
        android:layout_marginBottom="329dp"
        android:onClick="calves"
        android:translationZ="10dp"
        app:srcCompat="@drawable/calvesright" />

    <ImageView
        android:id="@+id/fullbody"
        android:layout_width="match_parent"
        android:layout_height="451dp"
        android:layout_below="@+id/toolbar"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="2dp"
        android:translationZ="2dp"
        tools:srcCompat="@drawable/fullbody2" />

    <ListView
        android:id="@+id/list"
        android:layout_width="411dp"
        android:layout_height="253dp"
        android:layout_below="@+id/toolbar"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="-1dp"
        android:layout_marginTop="422dp"
        android:layout_marginBottom="0dp"
        android:background="#E3E7F1" />

    <TextView
        android:id="@+id/emptyView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:fontFamily="@font/ubuntu"
        android:text="No Results"
        android:textColor="#52688F"
        android:textSize="20sp"
        android:visibility="gone" />
</RelativeLayout>